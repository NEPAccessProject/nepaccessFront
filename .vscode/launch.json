{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Edge",
            "port": 3000,
            "url": "http://localhost:3000",
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}//nepaccessFront",
            "preLaunchTask": "npm: start"
        },
        {
            "name": "Master - Attach to Edge",
            "port": 3001,
            "url": "http://localhost:3001",
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}//nepaccessFront-master//",
            "preLaunchTask": "npm: start"
        },
        {
            "type": "browse-lite",
            "request": "launch",
            "name": "Browse Lite: Launch",
            "url": "http://localhost:3000/",
            "webRoot": "${workspaceFolder}//nepaccessFront//"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}//nepaccessFront//"
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}//nepaccessFront//",
            "preLaunchTask": "npm: start",
            "cwd": "${workspaceFolder}//nepaccessFront//"
        },
        {
            "name": "Master - Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3001",
            "webRoot": "${workspaceFolder}//nepaccessFront-master//",
            "preLaunchTask": "npm: start",
            "cwd": "${workspaceFolder}//nepaccessFront-master//"
        },

        {
            "name": "Launch Edge Master",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:3001",
            "webRoot": "${workspaceFolder}//nepaccessFront-master//",
            "preLaunchTask": "npm: start",
            "cwd": "${workspaceFolder}//nepaccessFront-master//"
        },
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },     
    ]
}